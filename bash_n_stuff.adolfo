```{bash}
for f in rita_*
do
sql="\copy dirty_rita from $f with header csv;";
echo $sql
echo "-------------------------------"
date
/Library/PostgreSQL/9.1/bin/psql -U postgres bigdata_db -c "$sql"
echo "*******************************"
date
done
